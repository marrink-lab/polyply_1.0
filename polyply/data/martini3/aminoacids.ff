;Martini-IDP dihedral function7

;Modify BB bondlength to 0.360 8000 for coil
;Modify BBB angle to 137 25 for coil

;add SC-BB-BB-SC and BB-BB(-1)-BB(+1)-SC improper and exceptional BB-BB(GLY)-BB-SC1 dihedral distribution, give up the BBBS/SBBB definition derivative.
;add BBS and SBB angles with restricted bended potenial resembling -scfix

; add residue specific BBS/SBB angles
; add residue specific BB-BB(-1)-BB(+1)-SC improper dihedrals
; add Pro specific bondlength

[ macros ]
protein_resnames "GLY|ALA|CYS|VAL|LEU|ILE|MET|PRO|HYP|ASN|GLN|ASP|ASP0|GLU|GLU0|THR|SER|LYS|LYS0|ARG|ARG0|HIS|HISH|PHE|TYR|TRP"
protein_resnames_non_pro "GLY|ALA|CYS|VAL|LEU|ILE|MET|ASN|GLN|ASP|ASP0|GLU|GLU0|THR|SER|LYS|LYS0|ARG|ARG0|HIS|HISH|PHE|TYR|TRP"
